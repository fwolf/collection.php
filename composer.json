{
    "name": "fwolf/collection",
    "description": "Collection library",
    "type": "library",
    "keywords": [
        "array",
        "collection",
        "typed"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Fwolf",
            "email": "fwolf.aide+collection.php@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "symfony/yaml": "~3.4",
        "doctrine/instantiator": "~1.0.5",
        "phpdocumentor/reflection-docblock": "~4.3"
    },
    "note": [
        "symfony/yaml 4.0 require PHP 7.1.0 for public const",
        "doctrine/instantiator 1.1.x require PHP 7.0 for method return type",
        "phpdocumentor/reflection-docblock 5 require PHP 7.0 for declare strict"
    ],
    "autoload": {
        "psr-4": {
            "Fwolf\\Common\\Collection\\": "src/Collection/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FwolfTest\\Common\\Collection\\": "tests/Collection/"
        }
    }
}
